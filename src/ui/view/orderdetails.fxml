<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="ui.controller.OrderDetailsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Order" />
      <GridPane layoutX="184.0" layoutY="120.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Phone No." />
            <Label text="Table No." GridPane.rowIndex="1" />
            <TextField fx:id="phoneNo" GridPane.columnIndex="1" />
            <TextField fx:id="tableNo" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#submitClicked" text="Submit" />
      <Button layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
   </children>
</AnchorPane>
