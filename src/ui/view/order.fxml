<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.OrderController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Order" />
      <Button layoutX="180.0" layoutY="355.0" mnemonicParsing="false" onAction="#submitOrderClicked" text="Submit Order" />
      <ScrollPane fx:id="list" layoutX="21.0" layoutY="41.0" prefHeight="304.0" prefWidth="283.0" />
      <Button layoutX="462.0" layoutY="355.0" mnemonicParsing="false" onAction="#cancelOrderClicked" text="Cancel Order" />
      <TabPane layoutX="304.0" layoutY="41.0" prefHeight="296.0" prefWidth="283.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Starter">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" />
               </content>
          </Tab>
          <Tab text="Main">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <GridPane>
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label text="Chicken" />
                              <Label text="Beef" GridPane.rowIndex="1" />
                              <Label text="Bacon" GridPane.rowIndex="2" />
                              <Button mnemonicParsing="false" onAction="#addChicken" text="+" GridPane.columnIndex="1" />
                              <Button mnemonicParsing="false" onAction="#addBeef" text="+" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <Button mnemonicParsing="false" onAction="#addBacon" text="+" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab text="Dessert">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab text="Beverage">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <GridPane layoutX="20.0" layoutY="356.0" prefHeight="30.0" prefWidth="152.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="price" text="0.0" GridPane.columnIndex="1" />
            <Label text="Total price:" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
