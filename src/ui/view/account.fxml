<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.AccountController">
   <children>
      <ImageView fitHeight="408.0" fitWidth="601.0" layoutX="1.0">
         <image>
            <Image url="@../../images/background.jpg" />
         </image>
      </ImageView>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="21.0" prefWidth="165.0" text="Account" />
      <Button fx:id="logOut" layoutX="512.0" layoutY="9.0" mnemonicParsing="false" onAction="#logOutClicked" text="Log Out" />
      <HBox layoutX="15.0" layoutY="50.0" prefHeight="300.0" prefWidth="570.0">
         <children>
            <Button mnemonicParsing="false" onAction="#createAccountClicked" prefHeight="300.0" prefWidth="285.0" text="Create Account" />
            <Button mnemonicParsing="false" onAction="#editAccountClicked" prefHeight="300.0" prefWidth="285.0" text="Edit Account" />
         </children>
      </HBox>
      <Button layoutX="15.0" layoutY="355.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
   </children>
</AnchorPane>
